{
  "name": "byelide",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint2": "run-p lint:stage lint:style spellcheck",
    "lint": "run-p lint:style spellcheck",
    "prepare": "husky install",
    "lint:stage": "lint-staged",
    "commit": "cz-git",
    "commitlint": "commitlint --edit",
    "format": "prettier --write src/",
    "lint:style": "stylelint \"**/*.{vue,css,less,html}\"",
    "lint:style:fix": "stylelint \"**/*.{vue,css,less,html}\" --fix",
    "spellcheck": "cspell lint --dot --gitignore --color --cache --show-suggestions \"src/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\""
  },
  "lint-staged": {
    "*.{html,vue,css,sass,scss,ts}": "prettier --write",
    "*.{vue,ts}": "eslint --fix",
    "*.{vue,css,sass,scss,less}": "stylelint --fix",
    "*.{ts,tsx,js,jsx,vue,md,html,css,less,scss}": "cspell lint --no-progress --no-summary --show-suggestions"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/cz-commitlint": "^20.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "^4.3.1",
    "cspell": "^9.2.2",
    "cz-git": "^1.12.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "npm-run-all2": "^8.0.4",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1",
    "zx": "^8.8.5"
  }
}
